  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^3.1.1",
    "dotpress": "^1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
