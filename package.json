{
  "name": "@dotpress/auth",
  "version": "1.0.0",
  "description": "JWT Authentication plugin for Dotpress library",
  "author": "Renaud Guyon",
  "license": "MIT",
  "homepage": "https://dotpress.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/renoguyon/dotpress-auth-plugin"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
